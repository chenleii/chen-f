<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chen.f.common.mapper.CountryMapper">
    
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.chen.f.common.pojo.Country">
        <id column="ID" property="id" />
        <result column="LETTER_CODE2" property="letterCode2" />
        <result column="LETTER_CODE3" property="letterCode3" />
        <result column="NAME" property="name" />
        <result column="CHINESE_NAME" property="chineseName" />
        <result column="NUMERIC_CODE" property="numericCode" />
        <result column="AREA_CODE" property="areaCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        `ID`,
        `LETTER_CODE2`,
        `LETTER_CODE3`,
        `NAME`,
        `CHINESE_NAME`,
        `NUMERIC_CODE`,
        `AREA_CODE`
        
    </sql>

</mapper>
